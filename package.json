{
  "name": "react-google-photo",
  "version": "1.0.0",
  "description": "React lightbox component using the google photo style",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "prettier": "prettier --write \"**/*.{js,ts,tsx,css,scss,json,md}\"",
    "generate-props": "node scripts/generateReactPropsTable.js src/index.js"
  },
  "repository": "https://github.com/pradel/react-google-photo.git",
  "author": "Pradel LeÃÅo <pradel.leo@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "google",
    "photo",
    "image",
    "lightbox",
    "responsive"
  ],
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{md,js,ts,tsx,json,css,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "no-scroll": "^2.1.1",
    "react-minimalist-portal": "^2.2.0",
    "react-transition-group": "^4.4.1",
    "screenfull": "^5.0.2"
  },
  "devDependencies": {
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "husky": "4.2.5",
    "lint-staged": "10.2.2",
    "prettier": "2.0.5",
    "react": "16.13.1",
    "react-docgen": "2.20.1",
    "react-dom": "16.13.1",
    "tsdx": "0.13.2",
    "tslib": "1.11.2",
    "typescript": "3.8.3"
  }
}
