{
  "name": "react-google-photo",
  "version": "0.0.1",
  "main": "lib/index.js",
  "scripts": {
    "flow": "flow status",
    "build": "babel src --out-dir lib",
    "prepublish": "yarn run build",
    "lint": "eslint 'src' 'docs/src'",
    "test": "yarn lint && yarn flow && yarn build",
    "prettier":
      "prettier --write \"src/**/*.{js,ts,tsx}\" \"docs/src/**/*.{js,ts,tsx,css,scss,json}\"",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,json,css,scss}": ["prettier --write", "git add"]
  },
  "repository": "https://github.com/pradel/react-google-photo.git",
  "author": "Pradel LeÃÅo <pradel.leo@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "no-scroll": "^2.1.0",
    "react-jss": "^8.4.0",
    "react-minimalist-portal": "^2.1.1",
    "react-transition-group": "^2.3.0",
    "screenfull": "^3.3.2"
  },
  "devDependencies": {
    "@wertarbyte/react-props-md-table": "^1.1.1",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.69.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.2",
    "prettier": "^1.11.1",
    "react": "^16.1.0",
    "react-dom": "^16.1.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": ["flowtype", "prettier"],
    "extends": ["prettier", "plugin:flowtype/recommended"],
    "env": {
      "browser": true
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "keywords": ["react", "google", "photo", "image", "lightbox", "responsive"]
}
